    pre#htmlimport-code.
        _importHtml: function(href, resolve, reject){
            var el = document.createElement('link')
            el.onload = function(){ //IE9 min
                resolve()
            }
            el.onerror = function(){ 
                reject()
            }
            el.href = href
            el.rel = 'import'
            window.document['head'].appendChild(el)
        }

